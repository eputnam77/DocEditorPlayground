{
  "name": "doceditorplayground",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "next": "12.3.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "latest",
    "@tiptap/react": "latest",
    "@tiptap/starter-kit": "latest",
    "@toast-ui/react-editor": "latest",
    "@toast-ui/editor": "latest",
    "@editorjs/editorjs": "latest",
    "@editorjs/header": "latest",
    "@editorjs/list": "latest",
    "@editorjs/paragraph": "latest",
    "quill": "latest",
    "slate": "latest",
    "slate-react": "latest",
    "lexical": "latest",
    "@lexical/react": "latest",
    "@ckeditor/ckeditor5-react": "latest",
    "@ckeditor/ckeditor5-build-classic": "latest"
  },
  "devDependencies": {
    "typescript": "latest",
    "@types/react": "latest",
    "@types/node": "latest",
    "vitest": "latest",
    "@testing-library/react": "latest",
    "eslint": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "prettier": "latest",
    "@tailwindcss/postcss": "latest",
    "autoprefixer": "latest",
    "@playwright/test": "latest"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
